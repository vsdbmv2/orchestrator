    "@types/restify-cors-middleware": "^1.0.2",
    "@types/sha1": "^1.1.3",
    "@types/xml2js": "^0.4.11",
    "@types/xml2json": "^0.11.4",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-preset-minify": "^0.5.1",
