    "cors": "^2.8.5",
    "jsdom": "^17.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "md5": "^2.3.0",
    "mysql2": "^3.6.1",
    "node-cron": "^3.0.2",
    "puppeteer": "^21.1.1",
    "restify": "^11.1.0",
    "restify-cors-middleware": "^1.1.1",
    "restify-router": "^0.6.2",
    "sha1": "^1.1.1",
    "socket.io": "^4.2.0",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.33"
