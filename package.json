{
  "name": "@vsdbmv2/orchestrator",
  "version": "1.0.0",
  "description": "VSDBMv2 orchestrator server",
  "main": "server.ts",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "npm run migrate && node --env-file=.env --import=tsx --watch ./src/server.ts",
    "premigrate": "node --import=tsx --env-file=.env ./src/database/createSchema.ts",
    "db:setup": "npm run migrate && npm run seed",
    "migrate": "knex migrate:latest --knexfile ./knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.ts",
    "seed": "knex seed:run --knexfile ./knexfile.ts",
    "seed:rollback": "knex seed:rollback --knexfile ./knexfile.ts",
    "dev:test:subtype": "node --env-file=.env --import=tsx --watch test-get-subtype.ts",
    "start": "npm run migrate && node dist/server.js",
    "copyDownloader": "cp ./src/utils/DownloadAndUnpackIEDB.sh ./dist/utils",
    "build": "rm -rf dist 2> /dev/null && babel --extensions '.ts' src --out-dir dist && yarn copyDownloader && echo 'Build orchestrator successfully'",
    "lint": "eslint .",
    "lint:fix": "eslint --cache --fix \"src/**/*.{ts,js,json}\" ."
  },
  "author": "Helton Fabio",
  "license": "ISC",
  "dependencies": {
    "@vsdbmv2/mapping-library": "^0.0.76",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "jsdom": "^17.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^3.1.0",
    "md5": "^2.3.0",
    "mysql2": "^3.10.2",
    "node-cron": "^3.0.0",
    "restify-router": "^0.6.2",
    "sha1": "^1.1.1",
    "socket.io": "^4.2.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^16.2.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/knex": "^0.16.1",
    "@types/mailgun-js": "^0.22.12",
    "@types/md5": "^2.3.1",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^2.0.4",
    "@types/restify": "^8.5.2",
    "@types/restify-cors-middleware": "^1.0.2",
    "@types/sha1": "^1.1.3",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "^0.4.11",
    "@types/xml2json": "^0.11.4",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "tsx": "^4.7.0",
    "xml-js": "^1.6.11"
  }
}
