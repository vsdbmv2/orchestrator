{
  "name": "@vsdbmv2/orchestrator",
  "version": "1.0.0",
  "description": "VSDBMv2 orchestrator server",
  "main": "server.ts",
  "scripts": {
    "dev": "bun run ./src/server.ts",
    "start": "node dist/server.js",
    "copyDownloader": "cp ./src/utils/DownloadAndUnpackIEDB.sh ./dist/utils",
    "build": "rm -rf dist 2> /dev/null && bun build ./src/server.ts && yarn copyDownloader && echo 'Build orchestrator successfully'",
    "lint": "eslint .",
    "lint:fix": "eslint --cache --fix \"src/**/*.{ts,js,json}\" ."
  },
  "author": "Helton Fabio",
  "license": "ISC",
